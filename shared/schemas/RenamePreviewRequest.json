    "allow_renamed": {
      "default": false,
      "title": "Allow Renamed",
      "type": "boolean"
    },
    "config": {
      "anyOf": [
        {
          "$ref": "#/$defs/RenameConfig"
        },
        {
          "type": "null"
        }
      ],
      "default": null
